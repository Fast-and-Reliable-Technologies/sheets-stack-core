{
  "name": "@de44/sheets-stack-core",
  "version": "1.1.2",
  "description": "JS wrapper to simplify interactions with Google Sheets.",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc --project tsconfig.build.json",
    "predocs": "rimraf docs",
    "docs": "typedoc --out docs src/index.ts",
    "docs:serve": "serve docs -p 3080",
    "docs:deploy": "gh-pages -d docs",
    "test": "jest --coverage",
    "help": "cat package.json | jq '.scripts'"
  },
  "keywords": [],
  "repository": {
    "url": "https://github.com/Fast-and-Reliable-Technologies/sheets-stack-core"
  },
  "author": "David Esposito <david+npm@espo.email>",
  "license": "MIT",
  "dependencies": {
    "better-logging": "^5.0.0",
    "dotenv": "^16.0.3",
    "googleapis": "^111.0.0",
    "lodash": "^4.17.21",
    "lodash-query": "^1.4.2",
    "node-cache": "^5.1.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/lodash": "^4.14.194",
    "gh-pages": "^5.0.0",
    "jest": "^29.4.3",
    "rimraf": "^4.1.2",
    "serve": "^14.2.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.5",
    "typescript": "^5.0.4"
  }
}
